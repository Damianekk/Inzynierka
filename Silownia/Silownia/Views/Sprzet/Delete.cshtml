@model Silownia.Models.Sprzet

@{
    ViewBag.Title = "Sprzęt: usuń";
}
<h2>Czy na pewno chcesz usunąć sprzęt: @Model.Nazwa z sali @Model.Sala.Numer_sali?</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()


    <div class="divDark">
        <div class="divInner">
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.Sala.Silownia.Nazwa)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Sala.Silownia.Nazwa)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Sala.Numer_sali)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Sala.Numer_sali)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Nazwa)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Nazwa)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Data_zakupu)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Data_zakupu)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Cena_zakupu)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Cena_zakupu)
                </dd>

            </dl>
            <div>
                <input type="submit" value="Usuń" class="btn  btn-large btn-lg btn-block btn-danger" />
            </div>
        </div>
    </div>
}
@if (Request.UrlReferrer != null)
{
    <button class="btn btn-large btn-block btn-primary btn-lg btn-primary" onclick="history.go(-1); return false;">powrót</button>
}