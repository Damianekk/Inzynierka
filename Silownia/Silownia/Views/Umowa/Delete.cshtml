@model Silownia.Models.Umowa

@{
    ViewBag.Title = "Umowa: usuń";
}

<h2>Czy na pewno chcesz usunąć umowę klienta: @Model.Klient.imieNazwisko z siłownią @Model.Silownia.Nazwa?</h2>
<div>
    <h4>Umowa</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Silownia.Nazwa)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Silownia.Nazwa)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.DataPodpisania)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DataPodpisania)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.DataZakonczenia)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DataZakonczenia)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Cena)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Cena)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Usuń" class="btn btn-default" /> |
            @Html.ActionLink("Powrót do listy umów", "Index")
        </div>
    }
</div>
