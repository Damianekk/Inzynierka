@model Silownia.Models.Specjalizacja
@{
    ViewBag.Title = "Specjalizacja: usuń";
}
<h2>Czy na pewno chcesz usunąć specjalizację @Model.Nazwa?</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="divDark">
        <div class="divInner">
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.Nazwa)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Nazwa)
                </dd>
            </dl>

            <div>
                <input type="submit" value="Usuń" class="btn btn-lg btn-large btn-block btn-danger" />
            </div>
        </div>
    </div>
}
<button type="submit" class="btn btn-large btn-block btn-lg btn-primary" onclick="location.href='@Url.Action("Index", "Specjalizacja")';return false;">powrót do listy specjalizacji</button>

